<?xml version="1.0" ?>
<server name="server_1" class="org.jpos.q2.iso.QServer" logger="Q2">

    <attr name="port" type="java.lang.Integer">10000</attr>
    <attr name="maxSessions" type="java.lang.Integer">100</attr>

    <channel class="org.jpos.iso.channel.NACChannel"
             packager="org.jpos.iso.packager.GenericPackager"
             logger="Q2"
             header="6000000000">
        <property name="packager-config" value="cfg/packager/iso87ascii-binary-bitmap.xml" />
    </channel>

    <request-listener class="org.jpos.iso.IncomingListener" logger="Q2">
        <property name="queue" value="txnmgr" />
    </request-listener>

    <in>Server_1_NETWORK_OUT</in>
    <out>Server_1_NETWORK_IN</out>

</server>